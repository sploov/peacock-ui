    "lucide-react": "^0.454.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "three": "^0.182.0"
  },
  "peerDependencies": {
    "react": "^19.0.0-rc",
    "react-dom": "^19.0.0-rc",
    "tailwindcss": "^3.4.14"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.1",
    "@types/react": "^19.0.0-rc",
    "@types/react-dom": "^19.0.0-rc",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.47",
    "react": "^19.0.0-rc",
    "react-dom": "^19.0.0-rc",
    "storybook": "^8.4.1",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0",
    "vitepress": "^1.6.4",
    "vitest": "^2.1.4"
