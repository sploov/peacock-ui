{
  "name": "@peacock-ui/core",
  "version": "3.0.1",
  "description": "A high-fidelity, motion-first design system for React 19 featuring glassmorphism and WebGL acceleration.",
  "type": "module",
  "author": "Sploov",
  "license": "MIT",
  "keywords": [
    "react",
    "design-system",
    "ui-kit",
    "framer-motion",
    "threejs",
    "react-three-fiber",
    "glassmorphism",
    "motion-ui",
    "tailwind",
    "react-19"
  ],
  "homepage": "https://sploov.github.io/peacock-ui/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sploov/peacock-ui.git"
  },
  "bugs": {
    "url": "https://github.com/sploov/peacock-ui/issues"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/peacock-ui.umd.cjs",
  "module": "./dist/peacock-ui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/peacock-ui.js",
      "require": "./dist/peacock-ui.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vitepress dev docs",
    "build": "tsc && vite build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "deploy": "gh-pages -d docs/.vitepress/dist",
    "lint": "eslint . --ext ts,tsx",
    "release:alpha": "npm publish --tag alpha",
    "release": "npm publish"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
